{
  "name": "dark-electrea",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "buildAndUpload": "vite build && scp -r ./dist/* root@pet-server:/var/www/html && rm ./dist -R",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "roboto-fontface": "*",
    "timelite": "^1.0.0",
    "vue": "^3.4.26",
    "vue-country-flag-next": "^2.3.2",
    "vue-router": "^4.3.2",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-spinners": "^1.2.2",
    "vuetify": "^3.6.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.25.0",
    "sass": "^1.76.0",
    "vite": "^5.2.11",
    "vite-plugin-vuetify": "^2.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parser": "vue-eslint-parser",
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-mutating-props": [
        "off"
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false
        }
      ],
      "switch-colon-spacing": "error",
      "comma-spacing": "error",
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}